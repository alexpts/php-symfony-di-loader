{
    "name": "alexpts/symfony-di-loader",
    "description": "Simple loader for symfony DI container",
    "homepage": "https://github.com/alexpts/php-symfony-di-loader",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "DI",
        "Symfony",
        "Factory",
        "Container"
    ],
    "authors": [{
        "name": "Alexpts",
        "email": "alexpts3@gmail.com"
    }],
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "symfony/dependency-injection": "^6.3",
        "symfony/yaml": "^6.3",
        "symfony/config": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "PTS\\SymfonyDiLoader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PTS\\SymfonyDiLoader\\Unit\\": "test/unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "mikey179/vfsstream": "^1.6"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --config=test/phpunit.xml"
    }
}
