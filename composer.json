{
    "name": "alexpts/symfony-di-loader",
    "description": "Simple loader for symfony DI container",
    "homepage": "https://github.com/alexpts/php-symfony-di-loader",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "DI",
        "Symfony",
        "Factory"
    ],
    "authors": [{
        "name": "Alexpts",
        "email": "alexpts3@gmail.com"
    }],
    "minimum-stability": "stable",
    "require": {
        "php": "~7.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/config": "^4.2"
    },
    "autoload": {
        "psr-0": {
            "PTS\\SymfonyDiLoader": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PTS\\SymfonyDiLoader\\Unit\\": "test/unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mikey179/vfsStream": "^1.6"
    }
}
