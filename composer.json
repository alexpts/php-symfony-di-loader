{
    "name": "alexpts/symfony-di-loader",
    "description": "Simple loader for symfony DI container",
    "homepage": "https://github.com/alexpts/php-symfony-di-loader",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "DI",
        "Symfony",
        "Factory"
    ],
    "authors": [{
        "name": "Alexpts",
        "email": "alexpts3@gmail.com"
    }],
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": "~7.4",
        "ext-json": "*",
        "symfony/dependency-injection": "~5.1",
        "symfony/yaml": "^5.1",
        "symfony/config": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "PTS\\SymfonyDiLoader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PTS\\SymfonyDiLoader\\Unit\\": "test/unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "mikey179/vfsstream": "^1.6"
    }
}
